@model List<Dream.Model.TB_Admin>
@{
    ViewBag.Title = "管理员列表";
}
<form class="form-inline" action="/SystemManager/AdminList" method="post" onsubmit="document.getElementById('pageIndex').value=0;return true;">
    <br /> 
    <table class="table table-hover table-bordered">
        <thead>
            <tr>
                <th>#</th>
                <th>昵称</th>
                <th>登录名</th>
                <th>操作</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td>@item.UserName</td>
                    <td>
                        <a href="javascript:">编辑备注</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</form>